    "def read_table_item(table_name, pk_name, pk_value):\n",
    "    \"\"\"\n",
    "    Return item read by primary key.\n",
    "    \"\"\"\n",
    "    table = dynamodb.Table(table_name)\n",
    "    response = table.get_item(Key={pk_name: pk_value})\n",
    "\n",
    "    return response\n",
    "\n",
    "\n",
